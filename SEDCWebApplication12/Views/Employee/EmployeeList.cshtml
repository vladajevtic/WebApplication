
@model IEnumerable<EmployeeDTO>
@{ 
    ViewData["DisplayBTN"] = true;
}

<h1>@ViewBag.Title</h1>

<div class="container">
    <a asp-controller="Employee" asp-action="EmployeeCreate">Create New</a>
    <div class="card-columns">
        @foreach (var employee in Model)
        {

            <partial name="_EmployeePartialView" model="employee" view-data="ViewData"/>
        }
    </div>
</div>